{
  "name": "rey-web-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "NODE_OPTIONS='-r @newrelic/next' next dev",
    "start": "NODE_OPTIONS='-r @newrelic/next' next start",
    "lint": "next lint",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "test": "TZ=Asia/Jakarta jest --watchAll",
    "test:coverage": "TZ=Asia/Jakarta jest --coverage",
    "test:dev": "TZ=Asia/Jakarta jest --watchAll --coverage --maxWorkers=20%",
    "test:clear": "jest --clearCache"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "next": "14.0.3",
    "next-international": "1.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.10",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.2"
  }
}
